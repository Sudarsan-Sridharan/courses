{
  "name": "courses",
  "version": "1.0.0",
  "description": "Full Stack project with Java, Spring Boot, MongoDB, Groovy, React, Redux, ES6",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all -p start-message remove-dist",
    "start": "npm-run-all -p test:watch open:src lint:watch",
    "dockerStart": "npm-run-all -p test:watch open:dockerSrc lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dockerSrc": "babel-node tools/dockerServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "install-all": "npm install --loglevel=silent",
    "install-silent": "npm install &>/dev/null",
    "post-install": "npm-run-all -p clean-dist lint test",
    "prebuild": "npm run install-all &>/dev/null && npm run post-install",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Sergii Illichevskyi",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "0.15.3",
    "bootstrap": "3.3.7",
