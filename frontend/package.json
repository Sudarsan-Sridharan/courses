    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-simplemde-editor": "3.6.4",
    "redux": "3.6.0",
    "redux-auth-wrapper": "0.9.0",
    "redux-promise-middleware": "4.2.0",
    "redux-thunk": "2.1.0",
    "remarkable": "1.7.1",
    "reselect": "2.5.4",
    "styled-components": "1.1.2",
    "toastr": "2.1.2"
